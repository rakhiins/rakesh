a=int(input("enter the value of a"))
print("result of the right shift operators from 2 to 10 is")
print(a>>2)
print(a>>3)
print(a>>4)
print(a>>5)
print(a>>6)
print(a>>7)
print(a>>8)
print(a>>9)
print(a>>10)


